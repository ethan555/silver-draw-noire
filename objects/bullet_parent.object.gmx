for (i = 1; i &lt; seg_num; i ++) {
    //Draw the trail with its width
    px = trailx[i-1];
    py = traily[i-1];
    nx = trailx[i];
    ny = traily[i];
    d = point_direction(px,py,nx,ny) - 90;
    vx = lengthdir_x(trailw, d);
    vy = lengthdir_y(trailw, d);
    draw_vertex(px+vx, py+vy);
    draw_vertex(px-vx, py-vy);
    draw_vertex(nx+vx, ny+vy);
    draw_vertex(nx-vx, ny-vy);
}
