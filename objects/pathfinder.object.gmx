            <string>///Fill the collision grid
var xcells = ceil(room_width/gwidth);
var ycells = ceil(room_height/gheight);
grid_pathfinding = ds_grid_create(xcells,ycells);
for (var xi = 0; xi &lt; xcells; xi ++) {
    for (var yi = 0; yi &lt; ycells; yi++) {
        if (place_meeting(xi*gwidth, yi*gheight, wall)) {
            //There's a wall here, we can't go here.
            grid_pathfinding[# xi, yi] = -2;
        } else {
            var inst = instance_place(xi*gwidth, yi*gheight, stairs);
            if (inst != noone) {
                //There are stairs here
                if (inst.partner.y &lt; inst.y) {
                    //These stairs go up
                    grid_pathfinding[# xi, yi] = STAIRS_UP;
                } else {
                    //These stairs go down
                    grid_pathfinding[# xi, yi] = STAIRS_DOWN;
                }
            } else {
                // There is nothing interesting here
                grid_pathfinding[# xi, yi] = -1;
            }
        }
    }
}
</string>
